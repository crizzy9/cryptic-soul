    <li>Big Data</li>
    <li>Nix</li>
    <li>Python</li>
    <li>Java</li>
    <li>AWS</li>
    <li>PyTorch</li>
    <li>Prompt Engneering</li>
    <li>Go</li>
    <li>Lua</li>
    <li>React</li>
    <li>KIE</li>
    <li>Spring Boot</li>
    <li>ROS2</li>
    <li>Arduino</li>
    <li>JavaScript</li>
  </ul>
</BaseLayout>
